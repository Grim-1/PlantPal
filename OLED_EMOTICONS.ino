void setup() {
    display.begin(SH1106_I2C_ADDRESS, OLED_RESET);
    display.display();
    delay(2000); // Pause for 2 seconds on start
}

void loop() {
    display.clearDisplay();
    display.drawBitmap(0, 0, normal, 128, 64, WHITE); // Adjust width/height to match bitmap size
    display.display();
    delay(2000); // Show for 2 seconds

    display.clearDisplay();
    display.drawBitmap(0, 0, furious, 128, 64, WHITE);
    display.display();
    delay(2000);

    display.clearDisplay();
    display.drawBitmap(0, 0, cute, 128, 64, WHITE);
    display.display();
    delay(2000);

    display.clearDisplay();
    display.drawBitmap(0, 0, neutral, 128, 64, WHITE);
    display.display();
    delay(2000);
}

